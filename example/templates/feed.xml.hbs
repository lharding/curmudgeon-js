<?xml version="1.0" encoding="UTF-8"?> 
<rss version="2.0"
	xmlns:content="http://purl.org/rss/1.0/modules/content/"
	xmlns:wfw="http://wellformedweb.org/CommentAPI/"
	xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:atom="http://www.w3.org/2005/Atom"
	xmlns:sy="http://purl.org/rss/1.0/modules/syndication/"
	xmlns:slash="http://purl.org/rss/1.0/modules/slash/"
	> 
 
<channel> 
	<title>{{blog_title}}</title> 
	<atom:link href="http://{{blog_host}}{{url_prefix}}feed.xml" rel="self" type="application/rss+xml" /> 
	<link>http://{{blog_host}}</link> 
	<description>{{blog_description}}</description>
	<lastBuildDate>{{now}}</lastBuildDate> 
	<generator>curmudgeon-js v0.01</generator>
	<language>en</language> 
	<sy:updatePeriod>hourly</sy:updatePeriod> 
	<sy:updateFrequency>1</sy:updateFrequency>
	{{#posts}} 
		<item> 
			<title>{{title}}</title> 
			<link>http://{{../blog_host}}{{../url_prefix}}{{outfile}}</link> 
			<comments>http://{{../blog_host}}{{../url_prefix}}{{outfile}}#disqus_thread</comments> 
			<pubDate>{{date}} +0000</pubDate> 
			<dc:creator>{{../blog_author}}</dc:creator> 
			<category><![CDATA[Uncategorized]]></category> 

			<guid isPermaLink="false">{{outfile}}</guid> 
			<description><![CDATA[{{{head}}}]]></description> 
			<content:encoded><![CDATA[{{{body}}}]]></content:encoded> 
		</item> 
	{{/posts}}
	</channel> 
</rss> 
